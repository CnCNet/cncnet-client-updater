<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net7.0;net7.0-windows</TargetFrameworks>
    <UseWindowsForms Condition="'$(TargetFramework)' != 'net7.0'">true</UseWindowsForms>
    <UseWindowsForms Condition="'$(TargetFramework)' == 'net7.0'">false</UseWindowsForms>
    <ApplicationHighDpiMode Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">PerMonitorV2</ApplicationHighDpiMode>
    <OutputType>Exe</OutputType>
    <Title></Title>
    <Description>CnCNet Client Second-Stage Updater</Description>
    <Company>CnCNet</Company>
    <Product></Product>
    <Copyright>Copyright © CnCNet 2022</Copyright>
    <Trademark></Trademark>
    <Version>1.0.0.1</Version>
    <FileVersion>1.0.0.1</FileVersion>
    <InformationalVersion>1.0.0.1</InformationalVersion>
    <AssemblyName>SecondStageUpdater</AssemblyName>
    <RootNamespace>SecondStageUpdater</RootNamespace>
    <ComVisible>false</ComVisible>
    <CLSCompliant>false</CLSCompliant>
    <AnalysisLevel>preview-all</AnalysisLevel>
    <LangVersion>preview</LangVersion>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <Platforms>AnyCPU</Platforms>
    <ComVisible>false</ComVisible>
    <CLSCompliant>false</CLSCompliant>
    <ApplicationManifest Condition="$([MSBuild]::GetTargetPlatformIdentifier('$(TargetFramework)')) == 'windows'">app.manifest</ApplicationManifest>
  </PropertyGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net7.0'">
    <Compile Remove="MainForm.cs" />
    <Compile Remove="MainForm.Designer.cs" />
    <EmbeddedResource Remove="MainForm.resx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Rampastring.Tools" Version="2.0.4" />
    <PackageReference Include="StyleCop.Analyzers" Version="1.2.0-beta.435">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>